#include <bits/stdc++.h>

using namespace std;

const int MAXN = 50 + 10;

int a[MAXN][MAXN], x[MAXN][MAXN];

int main() {
    int t;
    scanf("%d", &t);
    while(t--) {
        int n, m;
        scanf("%d%d", &n, &m);
        for(int i=0; i<n; i++)
            for(int j=0; j<m; j++)  scanf("%d", a[i]+j);

        int ans = 0;
        for(int i=0; i<n; i++) {
            x[i][m-1] = a[i][m-1];
            for(int j=m-2; j>=0; j--) {
                x[i][j] = max(a[i][j], x[i][j+1]+1);
                printf("%d|\n", x[i][j]);
            }
            ans ^= a[i][0];
        }

        puts(ans ? "FIRST" : "SECOND");
    }

    return 0;
}
